    normal = img_as_float(img) # the normalization
    image_list_normalized.append(normal)  #Create a list of normalized full images 

task 8 - displaying the formula for MGVRI 

MGVRI = $\frac{(G^2 - R^2)}{(G^2 + R^2)}$ 

# task 9
# function to calculate MGVRI from a RGB image
np.seterr(invalid='ignore') # ignore 0/0 when dividing
def calc_mgvri(image):
  red_channel = image[:,:,0]
  green_channel = image[:,:,1]
  mgvri = np.divide(
    np.subtract((green_channel)**2, (red_channel)**2),
    np.add((green_channel)**2, (red_channel)**2))
  return mgvri
  

# task 10
calculated_mgvri_of_the_images = [] # Empty list to store calculated mgvri of the images
for img in image_list_normalized:
  calculated_mgvri = calc_mgvri(img) # using the function to calculate MGVRI from a RGB image
  calculated_mgvri_of_the_images.append(calculated_mgvri) # Create a list of calculated mgvri of the images


# task 12
calculated_mean_of_the_mgvri_images = [] # Empty list to store calculated mean of the mgvri images
for mgvri_image in calculated_mgvri_of_the_images:
  mean_mgvri = np.nanmean(mgvri_image) # Compute the mean of each MGVRI image, ignoring NaNs
  calculated_mean_of_the_mgvri_images.append(mean_mgvri) # Create a list of calculated mean of mgvri of the images


